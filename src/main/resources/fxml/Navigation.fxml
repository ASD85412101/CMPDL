<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox alignment="BOTTOM_CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.github.franckyi.cmpdl.controller.NavigationController">
    <children>
        <JFXButton onAction="#actionPrevious" styleClass="btn-text-primary" text="PREVIOUS">
            <font>
                <Font name="Roboto" size="16.0" />
            </font>
        </JFXButton>
        <JFXButton onAction="#actionNext" styleClass="btn-primary" text="NEXT">
            <font>
                <Font name="Roboto" size="16.0" />
            </font>
        </JFXButton>
    </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</HBox>
